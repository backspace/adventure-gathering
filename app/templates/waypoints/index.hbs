<div class="row">
  <div class="small-12 columns">
    <table>
      <thead>
        <tr>
          <th>
            Region
          </th>
        </tr>
      </thead>
      <tbody>
        {{#each this.model as |waypoint|}}
          <tr data-test-waypoint>
            <td data-test-name>
              {{waypoint.name}}
            </td>
            <td data-test-author>
              {{waypoint.author}}
            </td>
            <td data-test-region>
              {{waypoint.region.name}}
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>