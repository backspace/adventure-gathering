<div class='row'>
  <div class='small-12 columns'>
    <table>
      <thead>
        <tr>
          <th>Name</th>
        </tr>
      </thead>
      <tbody>
        {{#each model as |team|}}
          <tr class='team'>
            <td class='name'>{{team.name}}</td>
            <td class='risk-aversion'>{{team.riskAversion}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
